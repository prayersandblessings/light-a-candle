{
  "builds": [{ "src": "frontend/package.json", "use": "@now/next" }],
  "build": {
    "env": {
      "NEXT_PUBLIC_API_URL": "@api-url",
      "NEXT_PUBLIC_URL": "@next-url"
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "@api-url",
    "NEXT_PUBLIC_URL": "@next-url"
  },
  "routes": [
    {
      "src": "(.*)",
      "dest": "frontend$1"
    }
  ]
}
